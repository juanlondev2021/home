        var userAct = true;
        var index = 0;
        var sections =['FirstCustomer','SecondCustomer','LocationAndPayments', 'InstallationDate','Products', 'Summary','Analysis','Services','ServicesList', 'PaymentMethod'];
        var sectionsItems =['Finance','Credit Card','Checking', 'ACH', 'Check','Lease'];
        next(sections[0]);
        //document.getElementById('send').style.display = 'block';
        
        function next(x){
            document.getElementById(x).style.display = 'block';
            document.getElementById('title').style.display = 'block';
            for(var s in sections){
                if(sections[s] != x){
                    document.getElementById(sections[s]).style.display = 'none';
                }
            }
        }  
        function hideSection(x, y){
            document.getElementById(x).style.display = y;
        }  
        function nextItem(x){
            document.getElementById(x).style.display = 'block';
            for(var s in sectionsItems){
                if(sectionsItems[s] != x && sectionsItems[s] != 'Checking' && sectionsItems[s] != 'Check' ){
                    document.getElementById(sectionsItems[s]).style.display = 'none';
                }
            }
        }  
        M.AutoInit();
        document.addEventListener('DOMContentLoaded', function() {
            if(sessionStorage.getItem('section') != null || sessionStorage.getItem('section') != ''){
                next(sessionStorage.getItem('section'));
            }
            sessionStorage.setItem('section', '');
        });
        
