<aura:component >
    <aura:handler name="change" value="{!v.open}" action="{!c.doInit}" />    
    <aura:attribute name="open" type="Boolean" default="false" required="true"/>
    <!-- OBJECT -->
    <aura:attribute name="appointment" type="Object" required="true"/>
    <aura:attribute name="appoinmType" type="Object" required="true" />

    <!-- PICKLIST VALUES -->
    <aura:attribute name="picklistValues" type="list" required="true"/> 
    <aura:attribute name="apiNames" type="List" required="true"/>

    <aura:attribute access="private"  name="Existing_Keypad__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Do_you_have_a_water_filtration_system__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Pets__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Types_of_Pets__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Pest_loose_in_backyard__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Backyard_gate_locked__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Age_of_Current_AC_System__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Occupancy_of_House__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Type_of_Material__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Leak_Damage__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Frequency_Desired__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Sq_Ft_of_Project_Area__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Design__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Included_in_Landscape_Project__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Chemical_Type__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Area_of_Concrete_Surface__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Area_to_be_coated__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Existing_Coating__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Existing_Issue_with_current_door__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Type_of_Material_on_Current_Door__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Area_of_Concrete__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Frequency_Requested__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Floorplan__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Cabinets__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Bath_Floorplan__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="All_Decision_Makers__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Main_Area_of_Concern__c" type="List" default="[]" />
    <aura:attribute access="private"  name="Payment_Type__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Blinds__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Current_Window_Coverings__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Paint_Or_Stained__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Number_of_Rooms__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Utility_Provider__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Number_of_Windows__c" type="List" default="[]" /> 

    <!-- PICKLIST VALUES FOR RECORDTYPE-->
    <aura:attribute name="picklistValuesByRecordType" type="list" required="true"/> 
    <aura:attribute access="private"  name="Type_of_Service__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Project_Type__c" type="List" default="[]" /> 

    <!-- MULTIPLICKLIST Rooms_to_be_Installed__c-->
    <aura:attribute access="private"  name="Rooms_to_be_Installed__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Rooms_to_be_Installed_Values" type="List" default="[]" /> 
    <aura:attribute access="private"  name="isBedrooms" type="Boolean" default="false" />
    <aura:attribute access="private"  name="isBathrooms" type="Boolean" default="false" />

    <!-- MULTIPLICKLIST Project_Type__c-->
    <aura:attribute access="private"  name="Project_Type_Values" type="List" default="[]" />

    <!-- MULTIPLICKLIST Description_of_closet__c-->
    <aura:attribute access="private"  name="Description_of_closet__c" type="List" default="[]" />
    <aura:attribute access="private"  name="Description_of_closet_values" type="List" default="[]" />


    <!-- MULTIPLICKLIST Type_of_System_to_be_installed__c-->
    <aura:attribute access="private"  name="Type_of_System_to_be_installed__c" type="List" default="[]" />
    <aura:attribute access="private"  name="Type_of_System_to_be_installed_values" type="List" default="[]" />

    <!-- MULTIPLICKLIST Rooms_included_in_project__c-->
    <aura:attribute access="private"  name="Rooms_included_in_project__c" type="List" default="[]" /> 
    <aura:attribute access="private"  name="Rooms_included_in_project_values" type="List" default="[]" /> 

    <!-- SECTION APPOINTMENT INFO  -->
    <aura:attribute access="private"  name="Timeline_of_Intent__c" type="List" default="[]" /> 
    <aura:attribute name="account" type="Account" required="true"/>
    <aura:attribute name="appointmentSetter" type="string" description="PERSONEL ID"/> 


    <!-- APPOINTMENT DATETIME -->
    <aura:attribute access="private" name="appointmentDate" type="Date" />
    <aura:attribute access="private" name="appointmentTime" type="DateTime" /> 
    <aura:attribute access="private" name="appointmentTimeValues" type="List" default="[]" />
    <aura:attribute access="private" name="startDate" type="List" default="[]" />
    <aura:attribute access="private" name="endHour" type="Integer" />

    <!-- LOOKUP -->
    <aura:attribute name="isRenderDone" type="Boolean" default="false" />

    <!-- MODAL CONFIMATION -->
    <aura:attribute name="openConfimationCreateHomeService" type="Boolean" default="false" />

    <!-- Event Upsert Appointment -->
    <aura:registerEvent name="upsertAppointment" type="c:DiacoEventInterestsWebForm" />

    <!-- Event update Appointment -->
    <aura:registerEvent name="updateAppointment" type="c:DiacoEventHomeServicesFlow" />

    <!-- HEADER  -->
    <div class="slds-card slds-box">
    
        <lightning:layout multipleRows="true" class="slds-page-header slds-page-header--object-home slds-border_bottom slds-m-bottom--small">

            <lightning:layoutItem padding="horizontal-small">
                <lightning:icon iconName="standard:quick_text" />
            </lightning:layoutItem>

            <lightning:layoutItem padding="horizontal-small">
                <div class="page-section page-header ">
                    <h1 class="slds-text-heading--label">Qualifying</h1>
                    <h2 class="slds-text-heading--medium">                                
                        <lightning:formattedText value="{!v.appointment.Id != null ? v.appointment.Name : v.appoinmType.name}" />
                    </h2>
                </div>
            </lightning:layoutItem>

            <aura:if isTrue="{! and(v.appointment.Id != null, v.appointment.Home_Service__c != null) }">
                <lightning:layoutItem  alignmentBump="left" padding="horizontal-small">

                    <a class="slds-m-horizontal_small slds-button" href="{!'/lightning/r/' + v.appointment.Home_Service__c + '/view'}" target="_blank">
                        <div class="slds-visual-picker">
                            <span class="slds-visual-picker__figure slds-visual-picker__icon slds-align_absolute-center">
                                <lightning:icon iconName="custom:custom33" alternativeText="Home service" title="Home Service" />
                            </span>
                            <span class="slds-visual-picker__body">
                                <span class="slds-text-title">Home Service</span>
                            </span>
                        </div>
                    </a>

                </lightning:layoutItem>
            </aura:if>

        </lightning:layout>

    </div>
    <!-- HEADER  -->
        <!-- RT Home Automation -->
        <aura:if isTrue="{!v.appoinmType.name == 'Home Automation' }">

            <div class="slds-grid slds-wrap slds-gutters">
                
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Main Area of Concern" value="{! v.appointment.Main_Area_of_Concern__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Main_Area_of_Concern__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Main_Area_of_Concern__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Existing Keypad" value="{! v.appointment.Existing_Keypad__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Existing_Keypad__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Existing_Keypad__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>
                
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of Entry/Exit Doors" value="{!v.appointment.Number_of_Entry_Exit_Doors__c}" type="number" step='0' />
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Existing Contract" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Existing_Contract__c }"/>
                </div>
                
                <aura:if isTrue="{! v.appointment.Existing_Contract__c}">
                    <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                        <lightning:input aura:id='fieldId' label="Months Remaining" value="{!v.appointment.Months_Remaining__c}" type="number" step="0" required="fañse"/>
                    </div>
                </aura:if>
                
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Do you have a Doorbell Camera ? " 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Do_you_have_a_Doorbell_Camera__c }"/>
                </div>
                
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="RMR Verified" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.RMR_Verified__c }"/>
                </div>
                
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="$99 Activation Fee Verified" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.X99_Activation_Fee_Verified__c }"/>                
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Payment Type" value="{! v.appointment.Payment_Type__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Payment_Type__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Payment_Type__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of People in the home" value="{!v.appointment.Number_of_People_in_the_home__c}" type="number" step="0"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of people with their own cell phone" value="{!v.appointment.Number_of_cell_phone_owners__c}" type="number" step="0"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of of people over 12" value="{!v.appointment.Number_of_people_over_12__c}" type="number" step="0"/>
                </div>
                
            </div>

        </aura:if>
        <!-- RT Home Automation -->

        <!--RT Water Filtration -->
        <aura:if isTrue="{!v.appoinmType.name == 'Water Filtration'}">

            <div class="slds-grid slds-wrap slds-gutters">

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Already have a system" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Already_have_a_system__c }"/>                
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Pre plumbing" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Pre_plumbing__c }"/>                
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of People in the home" value="{!v.appointment.Number_of_People_in_the_home__c}" type="number" step="0"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Avg Monthly Spend on Bottled Water" value="{!v.appointment.Avg_Monthly_Spend_on_Bottled_Water__c}" type="number" step="0"/>
                </div>     
                    
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Do you have a water filtration system?" value="{! v.appointment.Do_you_have_a_water_filtration_system__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Do_you_have_a_water_filtration_system__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Do_you_have_a_water_filtration_system__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

            </div>

        </aura:if>
        <!--/RT Water Filtration -->

        <!--RT Solar -->
        <aura:if isTrue="{!v.appoinmType.name == 'Solar' }">

            <div class="slds-grid slds-wrap slds-gutters">
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Utility Provider" value="{! v.appointment.Utility_Provider__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Utility_Provider__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Utility_Provider__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Number of People in the home" value="{!v.appointment.Number_of_People_in_the_home__c}" type="number" step="0"/>
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="House Sq. Footage" value="{!v.appointment.House_Square_Footage_Text__c}"/>
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Avg Temperature AC Kept at" value="{!v.appointment.Avg_Temperature_AC_Kept_at__c}" type="number" step="0"/>
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Avg Electric Bill" value="{!v.appointment.Avg_Electric_Bill__c}"/>
                </div>
        
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id='fieldId' label="Credit Score" value="{!v.appointment.Credit_Score__c	}"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1">
                    <lightning:dualListbox  aura:id="fieldId" 
                                            name="ProjectType" 
                                            label="Project Type" 
                                            sourceLabel="Available" 
                                            selectedLabel="Selected" 
                                            options="{!v.Project_Type__c}" 
                                            value="{!v.Project_Type_Values}" 
                                            onchange="{!c.handlerProjectType}"/>
                </div>
        
            </div>
        </aura:if>
        <!--/RT Solar -->

        <!-- RT Pest -->
        <aura:if isTrue="{!v.appoinmType.name == 'Pest Control' }">

            <div class="slds-grid slds-wrap slds-gutters">

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Pets" value="{! v.appointment.Pets__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Pets__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Pets__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Types of Pets" value="{! v.appointment.Types_of_Pets__c	 }">
                        <aura:iteration items="{!v.Types_of_Pets__c	}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Types_of_Pets__c	 == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Pets loose in backyard" value="{! v.appointment.Pest_loose_in_backyard__c}">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Pest_loose_in_backyard__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Pest_loose_in_backyard__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Backyard gate locked" value="{! v.appointment.Backyard_gate_locked__c}">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Backyard_gate_locked__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Backyard_gate_locked__c	 == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1">
                    <lightning:dualListbox  aura:id="fieldId" 
                                            name="ProjectType" 
                                            label="Project Type" 
                                            sourceLabel="Available" 
                                            selectedLabel="Selected" 
                                            options="{!v.Project_Type__c}" 
                                            value="{!v.Project_Type_Values}" 
                                            onchange="{!c.handlerProjectType}"/>
                </div>
            
            </div>

        </aura:if>
        <!-- RT Pest -->

        <!-- RT HVAC -->
        <aura:if isTrue="{!v.appoinmType.name == 'HVAC' }">

            <div class="slds-grid slds-wrap slds-gutters">

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Age of Current AC System" value="{! v.appointment.Age_of_Current_AC_System__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Age_of_Current_AC_System__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Age_of_Current_AC_System__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-1">
