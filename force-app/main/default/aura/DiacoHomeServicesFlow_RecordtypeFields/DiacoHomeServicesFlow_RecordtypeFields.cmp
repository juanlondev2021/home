        <aura:if isTrue="{!v.appoinmType.name == 'Bathroom Remodel' }">

            <div class="slds-grid slds-wrap slds-gutters">

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Floorplan" value="{! v.appointment.Bath_Floorplan__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Bath_Floorplan__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Bath_Floorplan__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="New Cabinets Vanity" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.New_Cabinets_Vanity__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Move Install New Shower Bath" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Move_Install_New_Shower_Bath__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Update/Replace Countertops" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Bath_Update_Replace_Countertops__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Move/Install New Toilet" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Move_Install_New_Toilet__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Update/Replace Sinks" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Bath_Update_Replace_Sinks__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Update/Replace Flooring" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Bath_Update_Replace_Flooring__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Show tub combo conversion to shower" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Show_tub_combo_conversion_to_shower__c }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_large">
                    <lightning:input    type="toggle" 
                                        label="Bench Seat" 
                                        messageToggleActive='yes'
                                        messageToggleInactive="No"
                                        checked = "{! v.appointment.Bench_seat__c }"/>
                </div>


            </div>

        </aura:if>
        <!-- /RT  Bathroom Remodel -->

        <!--APPOINTMENT INFORMATION -->
        <div>
            <div class="slds-card slds-box">
                <lightning:layout class="slds-page-header slds-page-header--object-home slds-border_bottom slds-m-bottom--small">

                    <lightning:layoutItem padding="horizontal-small">
                        <lightning:icon iconName="standard:action_list_component" />
                    </lightning:layoutItem>
                    <lightning:layoutItem padding="horizontal-small">
                        <div class="page-section page-header ">
                            <h1 class="slds-text-heading--label">Appointment</h1>
                            <h2 class="slds-text-heading--medium">Information</h2>
                        </div>
                    </lightning:layoutItem>
    
                </lightning:layout>
            </div>
            <div class="slds-grid slds-wrap slds-gutters">
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2 slds-m-top_small">
                    <p class="slds-text-title" title="System Size">Account</p>
                    <lightning:button variant="base" label="{!v.account.Name}" title="Base action" onclick="{! c.handlerOpenAccount }"/>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Timeline of Intent" value="{! v.appointment.Timeline_of_Intent__c }">
                        <option value="">--None--</option>
                        <aura:iteration items="{!v.Timeline_of_Intent__c}" var="item" >
                            <option value="{!item.key}" selected="{! v.appointment.Timeline_of_Intent__c == item.key}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>

                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:input aura:id="fieldId" type="date" label="Appointment Date" value="{!v.appointmentDate}" onchange="{!c.handlerAppointmentDateTime}"/>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:select  aura:id='fieldId' label="Appointment Time" value="{! v.appointmentTime }"  onchange="{!c.handlerAppointmentDateTime}">
                        <option value="{!v.appointmentTime}"> <lightning:formattedText value="{!v.appointmentTime}" /> </option>
                        <aura:iteration items="{!v.appointmentTimeValues}" var="item" >
                            <option value="{!item.time}" selected="{! v.appointmentTime == item.time}">
                                {!item.value}
                            </option>
                        </aura:iteration>                                                                                    
                    </lightning:select>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <lightning:textarea aura:id='fieldId' label="Appointment Notes" placeholder="type here..." value="{!v.appointment.Appointment_Notes__c}"/>
                </div>
                <aura:if isTrue="{!v.appointmentSetter == null}">
                    <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                        <c:diacoLookup  objectAPIName="Personnel__c"                                         
                                        value="{!v.appointment.Appointment_Setter__c}"
                                        IconName="custom:custom47" 
                                        label="Appointment Setter"                                          
                                        required="false" 
                                        isRenderDone="{!v.isRenderDone}"/>
                    </div>
                </aura:if>
                <div class="slds-col slds-size_1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2">
                    <c:diacoLookup  objectAPIName="Personnel__c"                                         
                                    value="{!v.appointment.Lead_Review__c}"
                                    IconName="custom:custom47" 
                                    label="Lead Review"                                          
                                    required="false"
                                    isRenderDone="{!v.isRenderDone}"/>

                </div>
            </div>
        </div>

        <div class="slds-align_absolute-center slds-m-top_small">
            <lightning:button   variant="brand" 
                                label="{!v.appointment.Id != null ? 'Update' : 'New'}" 
                                title="upsert" 
                                iconName="{!v.appointment.Id != null ? 'utility:edit' : 'utility:new'}" 
                                iconPosition="left"
                                onclick="{! c.handlerUpdate }" />
        </div>
            
        <!--APPOINTMENT INFORMATION -->

    
</aura:component>