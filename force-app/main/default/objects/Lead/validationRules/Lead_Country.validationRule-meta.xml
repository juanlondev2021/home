<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_Country</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT( OR(ISPICKVAL( Lead_Country__c ,&quot;US&quot;),ISPICKVAL(Lead_Country__c,&quot;&quot;))))</errorConditionFormula>
    <errorMessage>Input Proper Lead Country</errorMessage>
</ValidationRule>
