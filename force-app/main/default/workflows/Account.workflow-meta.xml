    <rules>
        <fullName>Account%3A Update Hot Lead Created Checkbox on Verbal Consent Update</fullName>
        <actions>
            <name>Account_Update_Hot_Lead_Created_Field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Hot_Lead_Created__c Update</description>
        <formula>ISCHANGED(Verbal_Consent__c) &amp;&amp; (ISPICKVAL(Verbal_Consent__c, &apos;Yes&apos;) || ISPICKVAL(Verbal_Consent__c, &apos;Phone Only&apos;)) &amp;&amp; (Hot_Lead_Created__c = false) &amp;&amp; ( Not(ISBLANK(Respond_Date__c))) 
&amp;&amp; Not(ISCHANGED(Respond_Date__c)) &amp;&amp; (Inventory_Created__c=false)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
